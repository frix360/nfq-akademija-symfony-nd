{% extends 'layouts/layout.html.twig' %}

{% block body %}

    <div class="bg-light m-4">
        <table class="table">
            <tbody>
            <tr>
                <th scope="row">Projektas</th>
                <th scope="row">Studentas</th>
            </tr>
            <tr>
                <td>{{ app.request.query.get("project") }}</td>
                <td>{{ app.request.query.get("name") }}</td>
            </tr>
            </tbody>
        </table>
        {% if app.request.query.get("name") == 'Ignas Dailydė' %}
        <div class="alert alert-success" role="alert">
            Dešimt balų
        </div>
        {% else %}
        <div class="alert alert-warning" role="alert">
            Gal pasiseks kitą kartą
        </div>
        {% endif %}
    </div>

    <div class="m-4">
        <a class="text-light" href="/">Visi studentai</a>
    </div>
{% endblock %}